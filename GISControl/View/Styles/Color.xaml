<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Main color-->
    <SolidColorBrush x:Key="PrimaryColor" Color="#3E434C"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#3C3F41"/>

    <!--Accent color-->
    <SolidColorBrush x:Key="AccentColor" Color="#41516D"/>
    <SolidColorBrush x:Key="SecondaryAccentColor" Color="#45494A"/>

    <!--Text color-->
    <SolidColorBrush x:Key="TextColor" Color="#C7C8CA"/>
</ResourceDictionary>