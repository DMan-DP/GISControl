<Application x:Class="GISControl.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="View/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Styles/Color.xaml"/>
                <ResourceDictionary Source="View/Styles/Fonts.xaml"/>
                <ResourceDictionary Source="View/Styles/Icon.xaml"/>
                <ResourceDictionary Source="View/Styles/ButtonStyle.xaml"/>
                <ResourceDictionary Source="View/Styles/Style.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>